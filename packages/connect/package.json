    "@hyrious/esbuild-plugin-commonjs": "^0.2.2",
    "esbuild": "^0.19.4",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@alanshaw/pail": "^0.3.3",
    "@fireproof/core": "^0.15.0-dev",
