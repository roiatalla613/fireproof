{
  "name": "@fireproof/monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Live database for the web",
  "type": "module",
  "scripts": {
    "build": "pnpm run build:cjs; pnpm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "clean": "rm -rf node_modules dist",
    "start:react": "pnpm --filter @fireproof-example/react start",
    "start:solid": "pnpm --filter @fireproof-example/solid-js start",
    "test": "tsx node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "test:cjs": "cd dist/cjs && rm package.json && pnpm init && node ../../node_modules/jasmine/bin/jasmine --config=../../jasmine.cjs.json",
    "test:esm": "cd dist/esm && node --experimental-vm-modules ../../node_modules/jasmine/bin/jasmine.js --config=../../jasmine.esm.json",
    "serve": "npx serve-http -p 8080 tests/www",
    "format": "prettier .",
    "lint": "eslint"
  },
  "keywords": [
    "database",
    "JSON",
    "immutable",
    "IPLD",
    "CID",
    "UCAN"
  ],
  "contributors": [
    "J Chris Anderson",
    "Alan Shaw",
    "Travis Vachon",
    "Mikeal Rogers"
  ],
  "license": "Apache-2.0 OR MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.14.6",
    "c8": "^7.12.0",
    "eslint": "^9.5.0",
    "jasmine": "^5.1.0",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireproof-storage/fireproof.git"
  },
  "bugs": {
    "url": "https://github.com/fireproof-storage/fireproof/issues"
  },
  "homepage": "https://github.com/fireproof-storage/fireproof#readme",
  "dependencies": {
    "@adviser/cement": "^0.1.13",
    "@ipld/car": "^5.3.1",
    "@ipld/dag-cbor": "^9.2.0",
    "@ipld/dag-json": "^10.2.1",
    "@ipld/unixfs": "^3.0.0",
    "@netlify/blobs": "^7.3.0",
    "@peculiar/webcrypto": "^1.5.0",
    "@types/randombytes": "^2.0.3",
    "@ucanto/interface": "^10.0.1",
    "@web3-storage/clock": "^0.4.1",
    "@web3-storage/pail": "^0.6.0",
    "@web3-storage/w3up-client": "^15.0.0",
    "better-sqlite3": "^11.0.0",
    "charwise": "^3.0.1",
    "cross-fetch": "^4.0.0",
    "idb": "^8.0.0",
    "ipfs-unixfs-exporter": "^13.5.0",
    "js-base64": "^3.7.7",
    "multiformats": "^13.1.1",
    "p-limit": "^5.0.0",
    "partykit": "^0.0.107",
    "partysocket": "^1.0.1",
    "prolly-trees": "^1.0.4",
    "randombytes": "^2.1.0",
    "std-env": "^3.7.0",
    "uuidv7": "^1.0.0"
  }
}
