{
    "plugins": [
        "jsdoc-ts-utils",
        "plugins/markdown",
        "jsdoc-plugin-typescript"
    ],
    "typescript": {
        "moduleRoot": "packages/fireproof"
    },
    "opts": {
        "template": "node_modules/clean-jsdoc-theme"
    },
    "source": {
        "includePattern": ".(jsx|js|ts|tsx)$"
    },
    "sourceType": "module"
}